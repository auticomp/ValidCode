<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\PerfilUsuario" table="perfil_usuario">
    <id name="idPerfilUsuario" type="integer" column="id_perfil_usuario">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="perfilUsuario" type="string" column="perfil_usuario" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ativo" type="integer" column="ativo" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
