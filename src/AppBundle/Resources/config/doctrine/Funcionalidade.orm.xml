<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Funcionalidade" table="funcionalidade">
    <id name="idFuncionalidade" type="bigint" column="id_funcionalidade">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="url" type="string" column="url" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="titulo" type="string" column="titulo" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ativo" type="integer" column="ativo" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dtCadastro" type="datetime" column="dt_cadastro" nullable="true"/>
  </entity>
</doctrine-mapping>
