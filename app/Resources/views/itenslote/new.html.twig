{% extends 'base.html.twig' %}

{% block body %}

<div class="row">

	<div class="col-md-12">
		<h3>
			Home > <a href="{{ path('itenslote_index') }}">Listagem de Empresas</a> / <a
				href="">Cadastro de Empresa</a>
		</h3>
		<hr />
	</div>

	<div class="col-md-12 text-right">
		<a class="btn btn-primary btn-sm" href="{{ path('itenslote_index') }}"> <i
			class="fa fa-backward"></i> Voltar para a Lista
		</a>
	</div>

	<div class="col-md-12">
		<hr />
		<div class="col-lg-8">
			{{ form_start(form) }} 
			
				{{ form_widget(form) }}

			<div class="col-lg-12">
				<hr />
				<button class="btn btn-default btn-sm" type="reset" value="Limpar">
					<i class="fa fa-erase"></i> Limpar
				</button>
				<button class="btn btn-primary btn-sm" type="submit" value="Create"
					data-loading-text="Salvando...">
					<i class="fa fa-save"></i> Salvar
				</button>
			</div>

			{{ form_end(form) }}
		</div>


	</div>

</div>

   
{% endblock %}
